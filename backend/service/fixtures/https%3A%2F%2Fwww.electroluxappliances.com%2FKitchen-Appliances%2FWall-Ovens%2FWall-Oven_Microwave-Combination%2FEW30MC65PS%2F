        if (typeof GLOSSARIES !== 'undefined' && GLOSSARIES.length) {

                var list = $('#claims ul'),

                    number = 1;

                    for (var i = 0; i < GLOSSARIES.length; i++) {

                    var item = GLOSSARIES[i];

                    if (item.type.toLowerCase() == 'claim') {

                        var targets = $('.' + item.className.toLowerCase()).not('.no-glossary');

                        if (targets.length > 0) {

                            $('<li><sup>' + number + '</sup>' + item.text + '</li>').appendTo(list);

                            targets.append('<sup>' + number + '</sup>');

                            number++;

                        
                        }

                    
                    }

                
                    }

            
        }

        
        });

