<script>

var livePersonCheck = function () {

    var $items = document.getElementsByClassName('LPMcontainer'), $item;

    for (var i = $items.length; i--;) {

      if (($item = $items[i].parentElement) && $item.hasAttribute('id')) {

        if ($item.querySelectorAll('[data-lp-event]')[0])

          $item.style.display = 'block';

      
      }

    
    }

  
};

document.onreadystatechange = function () {

  if (document.readyState === "complete") {

      livePersonCheck();

    
  }

  
}

</script>



                



