<script type="text/javascript"> 

var recommendedScrollTrigger = function() {

      var triggerCall;

      var triggersOn = function () {

        $(window).on(Elux.Events.scroll + '.recommendedproducts', function(e) {

            if ($(window).scrollTop() >= triggerCall) {

            getRecommendedProducts(e);

          
            }

        
            });

            $(document).on('click.recommendedproducts', 'a[href="#productrecommended"]', getRecommendedProducts);

      
      };



    var $recommended = $('.block--recommended:visible');

    if ($recommended.length) 

    {

      triggerCall = $recommended.offset().top - 750;

      triggersOn();

    
    }

    

    function getRecommendedProducts(e) {

      $(document).off('click.recommendedproducts');

      $(window).off(Elux.Events.scroll + '.recommendedproducts');

        

            $('.loading-indicator').show();



            window.vm.Product.LoadRelatedProducts(

                'EW30MC65PS',

                function (data) { $('.loading-indicator').hide();  }
                );

      
    }

  
};



