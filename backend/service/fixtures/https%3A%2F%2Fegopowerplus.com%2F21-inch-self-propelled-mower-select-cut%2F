<script id="bv_ratings_script">
if (storeLocale.toLowerCase() === "en_au") {
  require(['jquery'],function ($) {
      $(window).load(function() {
                getRatingCount();

                setTimeout(function () {
                    getRatingCount();
                
                    }, 500);
            
          });

      function getRatingCount() {
                let numberOfReviews = BV.rating_summary.data[0].data.reviewSummary.numReviews;
                if (numberOfReviews) {
                    $('#ego-bv-main-rating-no').html(BV.rating_summary.data[0].data.reviewSummary.numReviews + " Reviews");
                
                    } else {
                    $('#ego-bv-main-rating-no').html("0 Reviews");
                
                    }
            
      }
        
      });
    
} else {
        var bv_ratings = document.getElementById("ego-bv-main-rating-stars");

        var bv_ratings_config = {
            attributes: true,
            attributeOldValue: true,
            characterData: true,
            characterDataOldValue: true,
            childList: true,
            subtree: true
        
        };

        var bv_ratings_loaded = false;

        function bv_ratings_callback(mutations) {
          for (var mutation of mutations) {
            if (mutation.type == "childList") {
                    let ratingsElem = bv_ratings.childNodes[0].childNodes[2].childNodes[1];
                    let numOfRatings = Number(ratingsElem.textContent.replace('(', '').replace(')', ''));
                    ratingsElem.textContent = '';

                    let reviewsAmount = document.getElementById('ego-bv-main-rating-no');
                    reviewsAmount.textContent = numOfRatings + ' Reviews';
                    bv_ratings_observer.disconnect();
                    document.getElementById('bv_ratings_script').remove();
                }
            }
        };

        var bv_ratings_observer = new MutationObserver(bv_ratings_callback);
        var changes = bv_ratings_observer.takeRecords();

        bv_ratings_observer.observe(bv_ratings, bv_ratings_config);
    }
