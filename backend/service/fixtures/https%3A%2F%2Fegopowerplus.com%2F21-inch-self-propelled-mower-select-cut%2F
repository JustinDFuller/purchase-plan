                                var $jsonp = (function() {
                                    var that = {};

                                    that.send = function(src, options) {
                                        var callback_name = options.callbackName || 'callback',
                                        on_success = options.onSuccess || function(){},
                                        on_timeout = options.onTimeout || function(){},
                                        timeout = options.timeout || 10; // sec

                                        var timeout_trigger = window.setTimeout(function(){
                                            window[callback_name] = function(){};
                                            on_timeout();
                                        
                                            }, timeout * 1000);

                                        window[callback_name] = function(data){
                                            window.clearTimeout(timeout_trigger);
                                            on_success(data);
                                        
                                        }

                                        var script = document.createElement('script');
                                        script.type = 'text/javascript';
                                        script.async = true;
                                        script.src = src;

                                        document.getElementsByTagName('head')[0].appendChild(script);
                                    
                                    }

                                    return that;
                                
                                    })();

                                $jsonp.send('https://ipinfo.io/?callback=callback', {
                                    callbackName: 'callback',
                                    onSuccess: function(response){
                                        country = response.country;
                                        setCookie('country', country, 365);
                                        countryRedirect(country);
                                    
                                    },
onTimeout: function(){
                                        setCookie('country', "US", 365);
                                        console.log('ipinfo Timeout');
                                    
},
                                    timeout: 5
                                
                                    });
                            
                                } );
                        
} else {
                            countryRedirect(country);
                        
}

function countryRedirect(location) {
  if (
                                window.location.hostname.indexOf('release') === -1
                                // && window.location.hostname.indexOf('local') === -1
                                && window.location.hostname.indexOf('integration') === -1
                                // && window.location.hostname.indexOf('staging') === -1
                                
     ) {
    switch(location.toLowerCase()) {
                                    case "au":
                                      if (windowHost.indexOf(".com.au/") > -1) {
                                            break;
                                        
                                      } else {
                                            window.location.href = "https://egopowerplus.com.au";
                                        
                                      }
                                    case "nz":
                                      if (windowHost.indexOf(".com.au/") > -1) {
                                            break;
                                        
                                      } else {
                                            window.location.href = "https://egopowerplus.com.au";
                                        
                                      }
                                    case "us":
                                      if (windowHost.indexOf(".com/") > -1) {
                                            break;
                                        
                                      } else {
                                        if (!getCookie('prevent-site-redirect')) {
                                                showEgoAUSiteConfirmationModal = true;
                                            
                                        }
                                            break;
                                        
                                      }
                                        break;
                                    case "ca":
                                        if (windowHost.indexOf(".com/") > -1) {
                                            break;
                                        
                                        } else {
                                          if (!getCookie('prevent-site-redirect')) {
                                                showEgoAUSiteConfirmationModal = true;
                                            
                                          }
                                            break;
                                        
                                        }
                                        break;
                                    case "gb":
                                        window.location.href = "http://egopowerplus.co.uk";
                                        break;
                                    case "at":
                                        window.location.href = "http://egopowerplus.at";
                                        break;
                                    case "be":
                                        window.location.href = "http://egopowerplus.be";
                                        break;
                                    case "fi":
                                        window.location.href = "http://egopowerplus.fi";
                                        break;
                                    case "fr":
                                        window.location.href = "http://egopowerplus.fr";
                                        break;
                                    case "de":
                                        window.location.href = "http://egopowerplus.de";
                                        break;
                                    case "ie":
                                        window.location.href = "http://egopowerplus.ie";
                                        break;
                                    case "it":
                                        window.location.href = "http://egopowerplus.it";
                                        break;
                                    case "nl":
                                        window.location.href = "http://egopowerplus.nl";
                                        break;
                                    case "es":
                                        window.location.href = "http://egopowerplus.es";
                                        break;
                                    case "ch":
                                        window.location.href = "http://egopowerplus.ch";
                                        break;
                                    case "za":
                                        window.location.href = "http://egopowerplus.co.za";
                                        break;
                                    default:
                                        break;
                                
    }
                            
  }
                        
}

function setCookie(cname, cvalue, exdays) {
                            var d = new Date();
                            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                            var expires = "expires="+d.toUTCString();
                            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
                        
}

function getCookie(cname) {
                            var name = cname + "=";
                            var ca = document.cookie.split(';');
                            for(var i = 0; i < ca.length; i++) {
                                var c = ca[i];
                                while (c.charAt(0) == ' ') {
                                    c = c.substring(1);
                                
                                }
                                if (c.indexOf(name) == 0) {
                                    return c.substring(name.length, c.length);
                                
                                }
                            
                            }
                            return "";
                        
}

function getUrlParameter(name) {
                            name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
                            var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
                            var results = regex.exec(location.search);
                            return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
                        
};
                        </script><!-- BEGIN GOOGLE VERIFICATION TAG -->
<!-- END GOOGLE VERIFICATION TAG -->
