                        if (getUrlParameter('preventredirect') === "true") { //Querystring to prevent country redirects (?preventredirect=true)
                            setCookie('country', 'preventredirect', 365);
                         } else if (getUrlParameter('preventredirect') === "false") { //Querystring to allow country redirects (?preventredirect=false)
                            setCookie('country', '', 365);
                         }

            /*-----------------*/
                        /* Redirect Login  */
