            <script>
            require(['jquery'],function ($) {
                    var container = document.getElementById("ego-videos-product-details-container");
                    let ego_media_collection = [];
                    if (ego_media_gallery) {
                    $.each(ego_media_gallery, function (index, data) {
                            let dataArray = data.cmspage_ids.split(',');
                            $.each(dataArray, function (pindex, pdata) {
                                if (storeLocale.toLowerCase() === "en_au" && pdata === "27") {
                                    return false;
                                
                                    } else {
                                    if (storeLocale.toLowerCase() === "default" && pdata === "27") {//EGO US
                                        ego_media_collection.push(data);
                                        return false;
                                    } else if (storeLocale.toLowerCase() === "en_au" && pdata === "277") {//EGO AU
                                        ego_media_collection.push(data);
                                        return false;
                                    }
                                
                                    }
                            
                                });
                        
                        });
                    
                    }
                    ego_media_collection.forEach(function(obj, index){
                        html = `<div class="ego-videos__item js-ego-grid-item">
                                    <div class="ego-videos__item-inner">
                                        <div class="ego-video-item">
                                            <a href="#" class="ego-video-item__img js-bg-parent js-play-video" data-src="${obj.videourl.replace("watch?v=","embed/")}">
                                                <img data-s-hidden="1" class="js-bg-hidden-child" src="${obj.filethumb}" alt="video thumbnail">
                                            </a>
                                            <h5 class="ego-video-item__title"><a data-src="${obj.videourl.replace("watch?v=","embed/")}" class="js-play-video" href="#">${obj.title}</a></h5>
                                            <p class="ego-video-item__time">${obj.videolength}</p>
                                        </div>
                                    </div>
                                </div>`;
                        container.innerHTML += html;
                    
                        });
                
                })();
            </script>
        </div>
        <a href="/video-gallery" class="ego-discussion__link ego-link" target="_blank">View Videos for All EGO Products</a>
    </div>
